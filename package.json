{
  "name": "unaxt-ui",
  "version": "0.0.5",
  "description": "UNA UI-Kit for developing web applications with NUXT & VUE",
  "repository": "your-org/my-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "style": "./dist/runtime/index.css",
      "import": "./dist/module.mjs"
    },
    "./runtime/*": "./dist/runtime/*",
    "./components/*": "./dist/runtime/components/ui/*"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ],
      "runtime/*": [
        "./dist/runtime/*"
      ],
      "components/*": [
        "./dist/runtime/components/*"
      ]
    }
  },
  "imports": {
    "#build/unaxt/*": "./.nuxt/unaxt/*.ts",
    "#build/unaxt.css": "./.nuxt/unaxt.css"
  },
  "style": "./dist/runtime/index.css",
  "files": [
    "README.md",
    "LICENSE",
    ".nuxt/unaxt",
    ".nuxt/unaxt.css",
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run test && npm run prepack && changelogen --release --patch && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.5",
    "@tailwindcss/vite": "^4.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.513.0",
    "reka-ui": "^2.3.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.5.0",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.5",
    "@nuxt/test-utils": "^3.19.1",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.28.0",
    "nuxt": "^3.17.5",
    "shadcn-nuxt": "2.2.0",
    "typescript": "~5.8.3",
    "vitest": "^3.2.2",
    "vue-tsc": "^2.2.10"
  }
}
